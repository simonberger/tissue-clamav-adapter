  "keywords": [
    "tissue",
    "anti-virus",
    "virus",
    "clamav",
    "security"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "simonberger/tissue": "^0.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "CL\\Tissue\\Adapter\\ClamAv\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "CL\\Tissue\\Adapter\\ClamAv\\Tests\\": "Tests/"
    }
